@Article{	  Bombin2012,
  Archiveprefix	= {arXiv},
  Abstract	= {Topological phases can be defined in terms of local
		  equivalence: two systems are in the same topological phase
		  if it is possible to transform one into the other by a
		  local reorganization of its degrees of freedom. The
		  classification of topological phases therefore amounts to
		  the classification of long-range entanglement. Such local
		  transformation could result, for instance, from the
		  adiabatic continuation of one system's Hamiltonian to the
		  other. Here, we use this definition to study the
		  topological phase of translationally invariant stabilizer
		  codes in two spatial dimensions, and show that they all
		  belong to one universal phase. We do this by constructing
		  an explicit mapping from any such code to a number of
		  copies of Kitaev's code. Some of our results extend to some
		  two-dimensional (2D) subsystem codes, including topological
		  subsystem codes. Error correction benefits from the
		  corresponding local mappings. In particular, it enables us
		  to use decoding algorithm developed for Kitaev's code to
		  decode any 2D stabilizer code and subsystem code.},
  Author	= {H Bombin and Guillaume Duclos-Cianci and David Poulin},
  Date-Added	= {2015-05-06 09:05:01 +0000},
  Date-Modified	= {2015-05-06 09:05:02 +0000},
  Eprint	= {1103.4606},
  Journal	= {New J. Phys.},
  Number	= {7},
  Pages		= {073048},
  Title		= {Universal topological phase of two-dimensional stabilizer
		  codes},
  Url		= {http://stacks.iop.org/1367-2630/14/i=7/a=073048},
  Volume	= {14},
  Year		= {2012},
  Bdsk-Url-1	= {http://stacks.iop.org/1367-2630/14/i=7/a=073048}
}

