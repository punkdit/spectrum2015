@Article{	  Bravyi2010,
  Abstract	= {We study zero-temperature stability of topological phases
		  of matter under weak time-independent perturbations. Our
		  results apply to quantum spin Hamiltonians that can be
		  written as a sum of geometrically local commuting
		  projectors on a D-dimensional lattice with certain
		  topological order conditions. Given such a Hamiltonian H\_0
		  we prove that there exists a constant threshold
		  \epsilon\&gt;0 such that for any perturbation V
		  representable as a sum of short-range bounded-norm
		  interactions the perturbed Hamiltonian H=H\_0+\epsilon V
		  has well-defined spectral bands originating from O(1)
		  smallest eigenvalues of H\_0. These bands are separated
		  from the rest of the spectrum and from each other by a
		  constant gap. The band originating from the smallest
		  eigenvalue of H\_0 has exponentially small width (as a
		  function of the lattice size).
		  
		  Our proof exploits a discrete version of Hamiltonian flow
		  equations, the theory of relatively bounded operators, and
		  the Lieb-Robinson bound.},
  Archiveprefix	= {arXiv},
  Author	= {Bravyi, Sergey and Hastings, Matthew and Michalakis,
		  Spyridon},
  Date-Added	= {2013-10-20 02:49:26 +0000},
  Date-Modified	= {2014-12-03 07:13:35 +0000},
  Doi		= {10.1063/1.3490195},
  Eprint	= {1001.0344},
  Journal	= {J. Math. Phys.},
  Month		= {Jan},
  Pages		= {093512},
  Title		= {Topological quantum order: stability under local
		  perturbations},
  Volume	= {51},
  Year		= {2010},
